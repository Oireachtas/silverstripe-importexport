{
    "name": "i-lateral/silverstripe-importexport",
    "description": "An upgrade to SilverStripe's bulk loading and exporting for PHP 8.1",
    "type": "silverstripe-vendormodule",
    "license": "MIT",
    "keywords": [
        "csv",
        "import",
        "bulkload",
        "export",
        "data"
    ],
    "authors": [
        {
            "name": "Jeremy Shipman",
            "email": "jeremy@burnbright.net"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Oireachtas/csv"
        }
    ],
    "require": {
        "php": ">=8.1",
        "silverstripe/framework": "^5.1",
        "silverstripe/admin": "^2.1",
        "silverstripe/assets": "^2.1",
        "goodby/csv": "dev-oireachtas"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "ilateral\\SilverStripe\\ImportExport\\": "src/",
            "ilateral\\SilverStripe\\ImportExport\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
